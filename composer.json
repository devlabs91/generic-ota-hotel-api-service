{
    "name": "devlabs91/generic-ota-hotel-api-service",
    "description": "A Generic Hotel Api Service, based on the OpenTravel (2003B-1.0) specifications.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.16",
        "ext-soap": "*",
        "wsdltophp/packagebase": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Devlabs91\\GenericOtaHotelApiService\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devlabs91\\GenericOtaHotelApiService\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "devlabs91/generic-hotel-api": "dev-master",
        "phpunit/phpunit": "^5.6",
        "symfony/web-server-bundle": "^4.2",
        "symfony/console": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/dotenv": "^4.2"
    }
}
