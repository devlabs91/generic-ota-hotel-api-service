																	<xs:documentation xml:lang="en">Provides full information on the specific charge.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="Vehicles" minOccurs="0">
																<xs:annotation>
																	<xs:documentation xml:lang="en">A collection of vehicles for which this charge applies. This is to be used when the charge e is not the same charge for all vehicle types.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="Vehicle" type="VehicleType" maxOccurs="99">
